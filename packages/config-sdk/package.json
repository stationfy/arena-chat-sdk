{
  "name": "@arena-im/config-sdk",
  "version": "1.0.0",
  "description": "Arena config-sdk is the official JavaScript config for Arena Chat, a service for building chat applications.",
  "author": "Arena",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stationfy/arena-config-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/stationfy/arena-config-sdk/issues"
  },
  "homepage": "https://github.com/stationfy/arena-config-sdk#readme",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.3",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.13.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "^2.3.2",
    "prettier-check": "2.0.0",
    "process": "^0.11.10",
    "rimraf": "3.0.2",
    "rollup": "2.52.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-jest": "^27.0.3",
    "typescript": "4.3.4"
  },
  "scripts": {
    "build": "run-s build:es5 build:esm",
    "build:es5": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:watch": "run-s build:watch:es5 build:watch:esm",
    "build:watch:es5": "tsc -p tsconfig.build.json -w --preserveWatchOutput",
    "build:watch:esm": "tsc -p tsconfig.esm.json -w --preserveWatchOutput",
    "clean": "rimraf dist esm",
    "link:yarn": "yarn link",
    "lint": "eslint . --ext .js,.ts",
    "pre-commit": "yarn lint-staged",
    "publish:registry": "yarn publish --access public",
    "size:check": "run-p size:check:bundle",
    "size:check:bundle": "cat dist/*.js | gzip -9 | wc -c | awk '{$1=$1/1024; print \"bundle: \",$1,\"kB\";}'",
    "test": "jest --forceExit --passWithNoTests",
    "test:jest:watch": "jest --watch --passWithNoTests",
    "test:watch": "run-s test:jest:watch --passWithNoTests"
  },
  "gitHead": "769b1a9a457e24ac16f0a90ea827b55a1a86f07b"
}
